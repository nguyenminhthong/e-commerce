{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseConfig": {
    "ConnectionString": "",
    "DataProvider": "sqlserver"
  },
  "ApiConfig": {
    "EnableSwagger": true,
    "EnableCache": true,
    "EnableDapper": true,
    "CacheTime": 60,
    "DistributedCacheType": "redis"
  },
  "RedisCacheConfig": {
    "ConnectionString": "localhost://9000"
  }
}
